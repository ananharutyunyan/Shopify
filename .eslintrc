 {
   "extends": "airbnb-base",
  
   "rules": {
      "semi": ["error", "never"],
      "no-console": ["error", { "allow": ["log", "warn"] }],
      "comma-dangle": ["error", "never"],
     "linebreak-style": ["error", "windows"],
    "max-len": ["warn", 100, 2, {
       "ignoreUrls": true,
       "ignoreComments": false,
       "ignoreRegExpLiterals": true,
       "ignoreStrings": true,
       "ignoreTemplateLiterals": true
     }]
   },
   "globals": {
     "window": true
   },
   "env": {
     "jest": true
   }
 }
