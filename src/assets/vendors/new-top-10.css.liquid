  .buttoncontinue {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    padding: 8px 21px;
     border: 1px solid #2072F0;
  border-radius: 2px;
  background: #d4d4d4;
  background: -webkit-gradient(linear, left top, left bottom, from(#d4d4d4), to(#818181));
  background: -moz-linear-gradient(top, #d4d4d4, #09A8E2);
  background: linear-gradient(to bottom, #2498F9, #09A8E2);
    text-shadow: #5c5c5c 1px 1px 1px;
    font: normal normal bold 12px arial;
    color: #ffffff;
    text-decoration: none;
}
.buttoncontinue:hover,
.buttoncontinue:focus {
    border: 1px solid #b8b8b8;
    background: #09A8E2;
    background: -webkit-gradient(linear, left top, left bottom, from(#fefefe), to(#9b9b9b));
    background: -moz-linear-gradient(top, #2498F9, #09A8E2);
    background: linear-gradient(to bottom, #2498F9, #09A8E2);
    color: #ffffff;
    text-decoration: none;
}
.buttoncontinue:active {
    background: #818181;
    background: -webkit-gradient(linear, left top, left bottom, from(#818181), to(#818181));
    background: -moz-linear-gradient(top, #2498F9, #09A8E2);
    background: linear-gradient(to bottom, #2498F9, #09A8E2);
}
  .accordion {
margin-bottom: 10px;
padding:10px;
border-top:#f0f0f0 1px solid;
background: #cc3366;
text-decoration:none;
color: #fff;
font-size:1.25em;
position:relative;
    border-radius: 5px;
      cursor: pointer;
}
.accordion-open {
background:#cc3366;
color: #fff;
}
.accordion-open span {
display:block;
position:absolute;
right:3px;
top:25%;
padding:10px;
}
.accordion-open span {
background:url('//cdn.shopify.com/s/files/1/0276/6855/t/14/assets/minus.png?4342')
center center no-repeat;
}

.accordion-close span:after {
display:block;
position:absolute;
right:3px;
  top: 25%;
  content: "+";
  font-size: 25px;
  font-weight: 500;
}
  
